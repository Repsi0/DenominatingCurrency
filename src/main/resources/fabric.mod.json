{
  "schemaVersion": 1,
  "id": "denominatingcurrency",
  "version": "${version}",

  "name": "Denominating Currency",
  "description": "A mod that adds 5 denominations of lower currency and 5 denominations of higher currency (and a little more). Not craftable by design, however this can be changed by editing the data yourself.",
  "authors": [
    "Repsi0"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/denominating-currency-fabric",
    "sources": "https://github.com/Repsi0/DenominatingCurrency",
	"issues": "https://github.com/Repsi0/DenominatingCurrency/issues"
  },

  "license": "GPL-3.0-or-later",
  "icon": "assets/denominatingcurrency/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.repsi0.denominatingcurrency.DenominatingCurrency"
    ]
  },
  "mixins": [
    "denominatingcurrency.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.16.x"
  },
  "suggests": {
    "another-mod": "*"
  }
}
